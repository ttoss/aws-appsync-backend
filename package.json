{
  "name": "@ttoss/aws-appsync-backend",
  "version": "0.2.0",
  "description": "AWS AppSync client for backend.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "prepare": "npm run build",
    "example:deploy": "aws cloudformation deploy --stack-name CoolChat --template-file ./example/cloudformation.yml",
    "example:api-key": "aws cloudformation describe-stacks --stack-name CoolChat --query 'Stacks[0].Outputs[?OutputKey == `ApiKey`].OutputValue' --output text",
    "example:url": "aws cloudformation describe-stacks --stack-name CoolChat --query 'Stacks[0].Outputs[?OutputKey == `Url`].OutputValue' --output text",
    "example:destroy": "aws cloudformation delete-stack --stack-name CoolChat",
    "example:query": "API_KEY=$(npm run -s example:api-key) URL=$(npm run -s example:url) npx ts-node example/example.ts query",
    "example:mutation": "API_KEY=$(npm run -s example:api-key) URL=$(npm run -s example:url) npx ts-node example/example.ts mutation",
    "example:subscription": "API_KEY=$(npm run -s example:api-key) URL=$(npm run -s example:url) npx ts-node example/example.ts subscription"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ttoss/aws-appsync-backend.git"
  },
  "keywords": [
    "aws-appsync",
    "nodejs"
  ],
  "author": {
    "name": "Pedro Arantes",
    "email": "arantespp@gmail.com",
    "url": "https://twitter.com/arantespp"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ttoss/aws-appsync-backend/issues"
  },
  "homepage": "https://github.com/ttoss/aws-appsync-backend#readme",
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "aws-appsync-auth-link": "^2.0.2",
    "aws-appsync-subscription-link": "^2.1.0",
    "cross-fetch": "^3.0.4",
    "graphql": "^14.0.0",
    "graphql-tag": "^2.10.3",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "@types/ws": "^7.2.4",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3",
    "yargs": "^15.3.1"
  }
}
